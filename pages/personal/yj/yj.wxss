/* pages/personal/yj/yj.wxss */
Page{
  background-color: #f5f5f5;
}

.yj {
  width: 100%;
  height: 200px;
  background-color: #fff;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
}

.yj-image-box {
  width: 80px;
  height: 80px;
  border-radius: 50%;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #ea9518;
}

.yj-image {
  width: 60px;
  height: 60px;
}

.yj-title {
  font-size: 30rpx;
}

.yj-number {
  font-size: 60rpx;
  color: #ea9518;
}