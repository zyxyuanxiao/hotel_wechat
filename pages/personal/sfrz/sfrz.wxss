/* pages/personal/sfrz/sfrz.wxss */
Page {
  background-color: #f5f5f5;
}
.zjsc-box {
  width: 90%;
  height: 180px;
  border: 1px solid #ea9518;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-top: 10px;
  margin-bottom: 10px;
}

.zjsc-image {
  width: 100%;
  height: 100%;
}

.zjsc-camera {
  width: 60px;
  height: 60px;
  border-radius: 50%;
  background-color: #ea9518;
  display: flex;
  justify-content: center;
  align-items: center;
}

.zjsc-camera-image {
  width: 40px;
  height: 40px;
}
.input-list {
  width: 100%;
}

.input-list-item {
  width: 100%;
  height: 40px;
  font-size: 30rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1px solid #d9d9d9;
}

.input-title {
  margin-left: 10px;
  color: #000;
}

.input-box {
  margin-right: 10px;
  text-align: right;
}
