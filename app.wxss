/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 10;
  box-sizing: border-box;
} 

.right-arrow {
  margin-left: 5px;
  width: 10px;
  height: 10px;
}

.common-btn {
  width: 90%;
  color: #ffffff;
  height: 35px;
  margin-top: 10px;
  font-size: 35rpx;
  background-color: #ea9518;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 5px;
}

.common-btn-disabled {
  width: 90%;
  color: #ffffff;
  height: 35px;
  margin-top: 10px;
  font-size: 35rpx;
  background-color: #8a8a8a;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 5px;
}

.common-btn-nofilling {
  width: 90%;
  color: #ea9518;
  height: 35px;
  margin-top: 10px;
  font-size: 35rpx;
  border: 1px solid #ea9518;
  background-color: #ffffff;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 5px;
}

.common-help {
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #bfbfbf;
  font-size: 25rpx;
  margin-top: 10px;
}

.common-help-icon {
  width: 14px;
  height: 14px;
  margin-right: 5px;
}

.panel-title {
  width: 100%;
  height: 30px;
  display: flex;
  align-items: center;
  background-color: #f5f5f5;
  border-left: 15px solid #ea9518;
  padding-left: 10px;
  font-size: 30rpx;
}